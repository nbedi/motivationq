{
  "manifest_version": 2,
  "name": "Motivation?",
  "description": "New tab extension inspired by the Motivation extension. Counts down not up.",
  "version": "0.1",

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "icons": { 
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png" 
  },

  "chrome_url_overrides" : {
      "newtab": "index.html"
  }
}